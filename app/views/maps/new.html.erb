


<% provide(:title, 'New Map') %>

<h1>New Map</h1>


<div class="row">
  <div class="span6 offset3">

    <%#= @map.name %>, <%#= @map.file_name %>


    <%= form_for(@map) do |f| %>

      <%#= render 'shared/error_messages' %>
    
      <%= f.label :name,                  "Name" %>
      <%= f.text_field :name %>

      <%= f.label :file_name,              "File name" %>
      <%= f.text_field :file_name %>

      <%#= f.label :type,                 "Type" %>
      <%#= f.text_field :type %>


      <%= f.submit "Create map", class: "btn btn-large btn-primary" %>

    <% end %>
    

  </div>
</div>

